
public class Day3 {

	public static void main(String[] args) {
		//***********************LOOPS*****************
		
		//FOR LOOP
		for(int var = 1 ; var < 10 ; var++) {
			System.out.println(var);		
			}
		System.out.println("Hello world");
		
		for(int num = 99 ; num >=1 ; num--) {
			System.out.println(num);
		}
		
	}

}
